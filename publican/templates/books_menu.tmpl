<script>$('<div>'+labels[current_product][current_version][current_book]["label"]+'</div>').insertBefore("#book_menu_list")</script>
<ul id="book_menu_list" class="hidden">
[%- FOREACH book_name = books %]
  <li [% IF lang != book_list.${book_name}.lang %]class="untranslated [% lang %]"[% END %]>
        <a href="[% host %]/[% lang %]/[% product %]/[% book_list.${book_name}.version %]/[% book_list.${book_name}.book %]/index.html">[% book_list.${book_name}.book_clean %][% IF lang != book_list.${book_name}.lang %] <span>[% trans_strings.Untranslated %]</span>[% END %]</a>
  </li>
[%- END %]
</ul>

