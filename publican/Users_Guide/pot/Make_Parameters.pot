#. Tag: title
#, no-c-format
msgid "Makefile Parameters"
msgstr ""

#. Tag: para
#, no-c-format
msgid "Every Book, Article or Book Set has a Makefile in its root directory. Parameters that can be set in a Book&#39;s Makefile are:"
msgstr ""

#. Tag: term
#, no-c-format
msgid "BOOKS"
msgstr ""

#. Tag: para
#, no-c-format
msgid "This defines the Books a Set contains. It is only useful for Book Sets."
msgstr ""

#. Tag: para
#, no-c-format
msgid "Default value: Undefined."
msgstr ""

#. Tag: term
#, no-c-format
msgid "BRAND"
msgstr ""

#. Tag: para
#, no-c-format
msgid "This parameter tells Publican which Brand to apply to a book&#39;s html and pdf output. Its default value is: common."
msgstr ""

#. Tag: term
#, no-c-format
msgid "BREW_DIST"
msgstr ""

#. Tag: para
#, no-c-format
msgid "BREW_DIST sets the Brew tag for desktop RPMs. Default: dist-5E-qu-candidate."
msgstr ""

#. Tag: term
#, no-c-format
msgid "CHUNK_FIRST"
msgstr ""

#. Tag: para
#, no-c-format
msgid "Controls chunking of first sections. 0 or 1. Default: 0."
msgstr ""

#. Tag: term
#, no-c-format
msgid "CHUNK_SECTION_DEPTH"
msgstr ""

#. Tag: para
#, no-c-format
msgid "Override the max section depth to chunk to. Default: 4."
msgstr ""

#. Tag: term
#, no-c-format
msgid "COMMON_CONFIG"
msgstr ""

#. Tag: para
#, no-c-format
msgid "Sets the path to Publican&#39;s configuration and common files, including Brand files. Default: <filename>/usr/share/publican</filename>."
msgstr ""

#. Tag: term
#, no-c-format
msgid "CONFIDENTIAL"
msgstr ""

#. Tag: para
#, no-c-format
msgid "The default value &mdash; 0 &mdash; is not confidential. To set a book to confidential, change this to 1."
msgstr ""

#. Tag: para
#, no-c-format
msgid "Setting this parameter to 1 places a large <emphasis>Confidential</emphasis> footer on each page."
msgstr ""

#. Tag: term
#, no-c-format
msgid "DEFAULT_LANGS"
msgstr ""

#. Tag: para
#, no-c-format
msgid "Languages that should be included in xi:include even if the translation is not complete. Used in update_includes. Default: Undefined."
msgstr ""

#. Tag: term
#, no-c-format
msgid "DESKTOP"
msgstr ""

#. Tag: para
#, no-c-format
msgid "Use the desktop CSS (1) or not (0). Default: 0"
msgstr ""

#. Tag: term
#, no-c-format
msgid "DOC_TYPE"
msgstr ""

#. Tag: para
#, no-c-format
msgid "Sets the sort of publication being produced: Book, Article or Set. Default value is Book. This paremeter has no relationship to the DOCTYPE declaration in a publication&#39;s XML files."
msgstr ""

#. Tag: term
#, no-c-format
msgid "DOC_URL"
msgstr ""

#. Tag: para
#, no-c-format
msgid "Sets the URL used for the link in the right-hand header of every html page. Also sets the URL entry in a publication&#39;s RPM spec file. Default: https://fedorahosted.org/publican"
msgstr ""

#. Tag: term
#, no-c-format
msgid "DT_OBSOLETES"
msgstr ""

#. Tag: para
#, no-c-format
msgid "A comma seperated list of desk top packages the package should obsolete. Default: undefined."
msgstr ""

#. Tag: term
#, no-c-format
msgid "EMBEDTOC"
msgstr ""

#. Tag: para
#, no-c-format
msgid "Embed the web navigation TOC in the html. 0 or 1. Default: 0."
msgstr ""

#. Tag: term
#, no-c-format
msgid "GENERATE_SECTION_TOC_LEVEL"
msgstr ""

#. Tag: para
#, no-c-format
msgid "Include a Table of Contents at the given section level. Valid values: 0 &ndash; 5. Default: 0."
msgstr ""

#. Tag: term
#, no-c-format
msgid "IGNORED_TRANSLATIONS"
msgstr ""

#. Tag: para
#, no-c-format
msgid "When building, replace any languages in this list with the source langauge text. This allows you to work on translations without shipping incomplete translations. Default: Undefined."
msgstr ""

#. Tag: term
#, no-c-format
msgid "OTHER_LANGS"
msgstr ""

#. Tag: para
#, no-c-format
msgid "Space seperated list of language locales. Default: Undefined."
msgstr ""

#. Tag: term
#, no-c-format
msgid "PRODUCT"
msgstr ""

#. Tag: para
#, no-c-format
msgid "The product this book is associated with. Default: Documentation."
msgstr ""

#. Tag: term
#, no-c-format
msgid "PROD_URL"
msgstr ""

#. Tag: para
#, no-c-format
msgid "Sets the URL used for the link in the left-hand header of every html page. Default: https://fedorahosted.org/publican"
msgstr ""

#. Tag: term
#, no-c-format
msgid "SHOW_REMARKS"
msgstr ""

#. Tag: para
#, no-c-format
msgid "Display contents of remark tags in output? Two possible values: 0 (off) or 1 (on). Off by default."
msgstr ""

#. Tag: term
#, no-c-format
msgid "TOC_SECTION_DEPTH"
msgstr ""

#. Tag: para
#, no-c-format
msgid "How many section levels should be included in the main Table of Contents. Default: 2."
msgstr ""

#. Tag: term
#, no-c-format
msgid "WEB_BREW_DIST"
msgstr ""

#. Tag: para
#, no-c-format
msgid "WEB_BREW_DIST sets the Brew tag for web RPMs. Default: docs-5E."
msgstr ""

#. Tag: term
#, no-c-format
msgid "WEB_OBSOLETES"
msgstr ""

#. Tag: para
#, no-c-format
msgid "A comma seperated list of web site packages the web package should obsolete. Default: Undefined."
msgstr ""

#. Tag: term
#, no-c-format
msgid "XML_LANG"
msgstr ""

#. Tag: para
#, no-c-format
msgid "Language of a publication&#39;s source xml. Default: en-US (US English)."
msgstr ""

#. Tag: term
#, no-c-format
msgid "USE_SAXON"
msgstr ""

#. Tag: para
#, no-c-format
msgid "Use saxon for xslt. Two possible values: 0 (off) or 1 (on). On by default."
msgstr ""

#. Tag: term
#, no-c-format
msgid "USE_XALAN"
msgstr ""

#. Tag: para
#, no-c-format
msgid "Test xalan for xslt. 0 or 1. Default: 0."
msgstr ""

